{
  "compilerOptions": {
    "baseUrl": "src",
    "outDir": "dist",
    "rootDirs": [
      "src"
    ],
    "incremental": true,
    "target": "es2021",
    "module": "commonjs",
    "strict": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "declaration": true
  }
}
